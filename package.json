{
  "name": "brightwheel",
  "version": "0.2.0",
  "description": "Build beautiful Electron user interfaces with Photon and Etch",
  "repository": "loranallensmith/brightwheel",
  "author": "Allen Smith &lt;loranallensmith@github.com&gt;",
  "license": "MIT",
  "keywords": [
    "electron",
    "etch",
    "user interface",
    "UI",
    "react"
  ],
  "main": "index.js",
  "jsnext:main": "index.es6.js",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb/base"
  },
  "dependencies": {
    "babel-runtime": "^6.25.0",
    "classnames": "^2.2.5",
    "etch": "^0.14.0",
    "photonkit": "^0.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.0",
    "coveralls": "^2.13.1",
    "del": "^3.0.0",
    "electron": "^1.6.11",
    "electron-mocha": "^4.0.0",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^3.5.0",
    "rollup": "^0.45.2",
    "rollup-plugin-babel": "^2.7.1",
    "sinon": "^2.4.1",
    "standard": "^10.0.2"
  },
  "scripts": {
    "lint": "node_modules/.bin/standard --recursive test && echo Linting passed",
    "test": "electron-mocha --renderer --compilers js:babel-register --require babel-polyfill",
    "test:cover": "babel-node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "build": "node tools/build",
    "prepublish": "npm run build"
  },
  "standard": {
    "parser": "babel-eslint",
    "global": [
      "beforeEach",
      "describe",
      "it",
      "expect",
      "requestAnimationFrame",
      "SVGSVGElement"
    ]
  }
}
